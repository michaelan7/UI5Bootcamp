<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core" >
	<Dialog
		title="Hello {/field/username}"
		draggable="true">
		<content>
			<core:Icon	src="sap-icon://hello-world"
						size="8rem"
						class="sapUiMediumMargin"/>
			<core:Icon	src="sap-icon://sap-ui5"
						size="8rem"
						class="sapUiMediumMargin"/>
		</content>
		<beginButton>

			<Button
				text= "{i18n>dialogCloseButtonText}"
				press= "onCloseDialog"/>
		</beginButton>
		
	</Dialog>
</core:FragmentDefinition>